{
  "env": {
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": false
      }
    ],
    "camelcase": [
      2,
      "always"
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "consistent-this": [
      2,
      "this"
    ],
    "eol-last": [
      2,
      "always"
    ],
    "func-names": [
      0
    ],
    "func-style": [
      0,
      "expression"
    ],
    "generator-star": [
      2,
      "end"
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "indent": [
      2,
      2
    ],
    "max-nested-callbacks": [
      2,
      3
    ],
    "new-cap": [
      2,
      {
        "newIsCap": true,
        "capIsNew": true
      }
    ],
    "new-parens": [
      2
    ],
    "no-array-constructor": [
      2
    ],
    "no-inline-comments": [
      2
    ],
    "no-lonely-if": [
      2
    ],
    "no-mixed-spaces-and-tabs": [
      2,
      "always"
    ],
    "no-multiple-empty-lines": [
      1,
      {
        "max": 2
      }
    ],
    "no-nested-ternary": [
      2
    ],
    "no-new": [
      0
    ],
    "no-new-object": [
      2
    ],
    "no-sequences": [
      0
    ],
    "no-spaced-func": [
      2
    ],
    "no-ternary": [
      0
    ],
    "no-trailing-spaces": [
      2
    ],
    "no-underscore-dangle": [
      0
    ],
    "no-wrap-func": [
      2
    ],
    "no-var": [
      0
    ],
    "one-var": [
      0
    ],
    "operator-assignment": [
      0
    ],
    "padded-blocks": [
      0
    ],
    "quote-props": [
      2,
      "as-needed"
    ],
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "never"
    ],
    "semi-spacing": {
      "before": false,
      "after": true
    },
    "sort-vars": [
      2,
      {
        "ignoreCase": true
      }
    ],
    "space-after-keywords": [
      2,
      "always"
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "space-before-function-parentheses": {
      "anonymous": "always",
      "named": "never"
    },
    "space-in-brackets": [
      0,
      "never",
      {
        "arraysInArrays": false,
        "arraysInObjects": false,
        "singleValue": false,
        "objectsInArrays": false,
        "objectsInObjects": false,
        "propertyName": false
      }
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "space-infix-ops": [
      2,
      "always"
    ],
    "space-return-throw-case": [
      2,
      "always"
    ],
    "space-unary-ops": [
      2,
      {
        "words": true,
        "nonwords": false
      }
    ],
    "spaced-line-comment": [
      2,
      "always"
    ],
    "wrap-regex": [
      2,
      "never"
    ]
  }
}
