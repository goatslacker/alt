{
  "name": "alt",
  "version": "0.14.2",
  "description": "A flux implementation",
  "main": "dist/alt.js",
  "dependencies": {
    "es-symbol": "^1.1.1",
    "eventemitter3": "^0.1.6",
    "flux": "^2.0.1",
    "object-assign": "^2.0.0"
  },
  "devDependencies": {
    "babel": "^4.6.6",
    "babel-eslint": "^1.0.14",
    "babelify": "^5.0.4",
    "browserify": "^9.0.3",
    "chai": "^2.1.0",
    "coveralls": "^2.11.2",
    "eslint": "^0.16.1",
    "istanbul": "^0.3.5",
    "mocha": "^2.1.0",
    "sinon": "^1.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/goatslacker/alt.git"
  },
  "scripts": {
    "build": "npm run build-alt; npm run build-alt-runtime; npm run build-alt-browser; npm run build-alt-browser-with-addons",
    "build-alt": "babel ./src/alt.js > dist/alt.js",
    "build-alt-runtime": "babel --external-helpers src/alt.js > dist/alt-with-runtime.js",
    "build-alt-browser": "NODE_ENV=production browserify src/alt.js -t babelify --outfile dist/alt-browser.js --standalone Alt",
    "build-alt-browser-with-addons": "NODE_ENV=production browserify src/alt-with-addons.js -t babelify --outfile dist/alt-browser-with-addons.js --standalone Alt",
    "coverage": "istanbul cover _mocha -- -u exports -R spec --compilers js:babel/register --require babel/external-helpers test",
    "lint": "eslint src mixins utils",
    "prepublish": "npm run test-build",
    "test": "npm run build-alt-runtime; npm run tests-all",
    "test-build": "npm run build; npm run tests-all",
    "test-browser": "browserify test/browser/index.js -t babelify --outfile test/browser/tests.js",
    "tests-all": "mocha -u exports -R spec --compilers js:babel/register --require babel/external-helpers test"
  },
  "keywords": [
    "alt",
    "es6",
    "flow",
    "flux",
    "react",
    "unidirectional"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "node": true
    },
    "rules": {
      "brace-style": [2, "1tbs", { "allowSingleLine": false }],
      "camelcase": [2, "always"],
      "comma-spacing": [2, { "before": false, "after": true }],
      "comma-style": [2, "last"],
      "consistent-this": [2, "this"],
      "eol-last": [2, "always"],
      "func-names": [0],
      "func-style": [0, "expression"],
      "generator-star": [2, "end"],
      "key-spacing": [2, {
        "beforeColon": false,
        "afterColon": true
      }],
      "indent": [2, 2],
      "max-nested-callbacks": [2, 3],
      "new-cap": [2, { "newIsCap": true, "capIsNew": true }],
      "new-parens": [2],
      "no-array-constructor": [2],
      "no-inline-comments": [2],
      "no-lonely-if": [2],
      "no-mixed-spaces-and-tabs": [2, "always"],
      "no-multiple-empty-lines": [1, { "max": 2 }],
      "no-nested-ternary": [2],
      "no-new": [0],
      "no-new-object": [2],
      "no-sequences": [0],
      "no-spaced-func": [2],
      "no-ternary": [0],
      "no-trailing-spaces": [2],
      "no-underscore-dangle": [0],
      "no-wrap-func": [2],
      "no-var": [0],
      "one-var": [0],
      "operator-assignment": [0],
      "padded-blocks": [0],
      "quote-props": [2, "as-needed"],
      "quotes": [2, "single"],
      "semi": [2, "never"],
      "semi-spacing": { "before": false, "after": true },
      "sort-vars": [2, { "ignoreCase": true }],
      "space-after-keywords": [2, "always"],
      "space-before-blocks": [2, "always"],
      "space-before-function-parentheses": {"anonymous": "always", "named": "never"},
      "space-in-brackets": [0, "never", {
        "arraysInArrays": false,
        "arraysInObjects": false,
        "singleValue": false,
        "objectsInArrays": false,
        "objectsInObjects": false,
        "propertyName": false
      }],
      "space-in-parens": [2, "never"],
      "space-infix-ops": [2, "always"],
      "space-return-throw-case": [2, "always"],
      "space-unary-ops": [2, { "words": true, "nonwords": false }],
      "spaced-line-comment": [2, "always"],
      "wrap-regex": [2, "never"]
    }
  },
  "author": "Josh Perez <josh@goatslacker.com>",
  "license": "MIT"
}
